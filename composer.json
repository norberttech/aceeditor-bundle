{
    "name": "norberttech/aceeditor-bundle",
    "type": "symfony-bundle",
    "description": "Bundle that integrate excellent JavaScript ace editor into Symfony Form.",
    "keywords": ["ace", "code", "editor", "symfony", "form", "bundle", "javascript"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "twig/twig": "^1.34|^2.0",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/form": "^3.0|^4.0",
        "symfony/twig-bridge": "^3.0|^4.0",
        "symfony/asset": "^3.0|^4.0",
        "symfony/expression-language": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": { "Norzechowicz\\AceEditorBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
